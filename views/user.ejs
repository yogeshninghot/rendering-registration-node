<body>
   <div class="container">
    
    <div class="row justify-content-center">
        <div class="col-md-5 ">
         
            
    
            <!-- <img src="webi.jpg" class="mt-4 mb-1" alt="shiva"> -->
            <h1 class="fw-bold fs-2 mt-5 mb-2">User Student Data Added</h1>
           <% for(let i=0;i<students.length; i++) { %>
             <div class="card my-3 p-3 d-flex">
                <div class="d-flex">
                    <div> <%= students[i].name %> </div>
                    <div class="mx-3"> <%= students[i].email %> </div>
                </div>
             </div>
            
             <%}%>
        </div>
    </div>

   </div>
   
